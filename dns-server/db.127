$TTL	1w
@	IN	SOA	dns1.example.com. hostmaster.example.com. (
			3		; Serial
			1w 		; Refresh
			1d		; Retry
			28d		; Expire
			1w) 		; Negative Cache TTL
			 
; name servers - NS records
		IN	NS	dns1.example.com.

; PTR Records
3.2.0.10.in-addr.arpa. 	IN	PTR	dns1.example.com.
2.2.0.10.in-addr.arpa. 	IN	PTR	dhcp.example.com.
4.2.0.10.in-addr.arpa. 	IN	PTR	ds1.example.com.
